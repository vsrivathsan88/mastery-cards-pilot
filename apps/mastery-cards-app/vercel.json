{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd native-audio-function-call-sandbox && npm run build",
  "devCommand": "cd native-audio-function-call-sandbox && npm run dev",
  "installCommand": "npm install && cd native-audio-function-call-sandbox && npm install",
  "outputDirectory": "native-audio-function-call-sandbox/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ]
}
